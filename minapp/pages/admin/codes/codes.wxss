page {
  background-color: #f1f1f1;
}
.preview-wrap {
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx 30rpx 0;
  padding-bottom: calc(110rpx + constant(safe-area-inset-bottom));
  padding-bottom: calc(110rpx + env(safe-area-inset-bottom));
}
.weui-form-preview + .weui-form-preview {
  margin-top: 30rpx;
}
.weui-loadmore_line .weui-loadmore__tips {
  background-color: #f1f1f1;
}
.haircut-flex {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  margin: 30rpx 0;
}
.haircut-flex .weui-flex__item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 30rpx 0;
  background-color: #fff;
  border-radius: 16rpx;
  font-size: 30rpx;
  color: #333;
}
.haircut-flex .weui-flex__item + .weui-flex__item {
  margin-left: 30rpx;
}
.fixed-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  /* height: 110rpx; */
  background-color: #fff;
  padding-bottom: calc(20rpx + constant(safe-area-inset-bottom));
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
  box-shadow: 0 0 30px 4px rgba(0, 0, 0, .1);
}
.fixed-bottom .weui-btn {
  margin-top: 20rpx;
}
