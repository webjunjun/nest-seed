<view class="not-tab-page">
  <view class="mine-bg-box">
    <image class="mine-bg" mode="widthFix" src="{{mineBg}}"></image>
  </view>
  <view class="mine-main">
    <view class="user-box">
      <image class="avatar" src="{{avatarUrl}}"></image>
      <view>{{realName}}</view>
      <view class="cellphone">{{cellphone}}</view>
    </view>
    <view class="mine-diner-wrap">
      <view class="mine-diner-inner">
        <view class="mdi-title">我的就餐记录</view>
        <view class="mdi-list">
          <view wx:for="{{list}}" wx:key="index" class="mdi-item mdi-today">
            <view class="mdi-left">
              <image src="{{avatarUrl}}"></image>
            </view>
            <view class="mdi-right">
              <view class="mdi-date">{{item.newDinerDate}} 就餐记录</view>
              <view>
                <view class="mdi-badge mdi-zao" wx:if="{{item.morning === 1}}">早餐</view>
                <view class="mdi-badge mdi-zhong" wx:if="{{item.midday === 1}}">中餐</view>
                <view class="mdi-badge mdi-wan" wx:if="{{item.evening === 1}}">晚餐</view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>
